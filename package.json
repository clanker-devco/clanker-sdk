{
  "name": "clanker-sdk",
  "version": "1.6.0",
  "description": "A lightweight TypeScript SDK for interacting with the Clanker API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "example:deploy": "ts-node examples/token-deployment.ts",
    "example:rewards": "ts-node examples/rewards-and-fees.ts",
    "example:market": "ts-node examples/market-data.ts",
    "example": "npm run example:deploy && npm run example:rewards && npm run example:market"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clanker-devco/clanker-sdk.git"
  },
  "homepage": "https://github.com/clanker-devco/clanker-sdk#readme",
  "bugs": {
    "url": "https://github.com/clanker-devco/clanker-sdk/issues"
  },
  "keywords": [
    "clanker",
    "api",
    "sdk",
    "typescript",
    "blockchain",
    "tokens",
    "web3"
  ],
  "author": "Clanker Team",
  "license": "ISC",
  "dependencies": {
    "@duneanalytics/client-sdk": "^0.2.5",
    "axios": "^1.6.7"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "dotenv": "^16.4.7",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
